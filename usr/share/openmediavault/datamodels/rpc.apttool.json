[{
	"type": "rpc",
	"id": "rpc.apttool.setpackage",
	"params": {
		"type": "object",
		"properties": {
			"uuid": {
				"type": "string",
				"format": "uuidv4",
				"required": true
			},
			"packagename": {
				"type": "string",
				"required": true
			},
			"backports": {
				"type": "boolean",
				"required": true
			}
		}
	}
},{
	"type": "rpc",
	"id": "rpc.apttool.docommand",
	"params": {
		"type": "object",
		"properties": {
			"packagename": {
				"type": "string",
				"required": true
			},
			"command": {
				"type": "string",
				"enum": [ "install", "remove", "purge", "hold", "unhold" ],
				"required": true
			},
			"backports": {
				"type": "string",
				"required": true
			}
		}
	}
